<!-- Put site-specific property overrides in this file. -->

<configuration>

<property>
<name>simulator.logpath</name>
<value>/home/reidchan/Desktop/FilterResult.txt</value>
<description>the log's absolute path in the file system</description>
</property>

<property>
<name>simulator.groupby</name>
<value>filename</value>
<description>the record log's groupby: "filename" or "timesequence"</description>
</property>

<property>
<name>simulator.opr.filter</name>
<value>irp</value>
<description>opr filter</description>
</property>

<property>
<name>simulator.irp.filter</name>
<value>all</value>
<description>irp filter</description>
</property>

<property>
<name>simulator.majorop.filter</name>
<value>read,write</value>
<description>major operation filter</description>
</property>

<property>
<name>simulator.status.filter</name>
<value>success</value>
<description>operation status filter</description>
</property>

<property>
<name>simulator.keyword.filter</name>
<value>\Device\HarddiskVolume2\Users\test\AppData\Local\Temp\WB_TEST-PC1_test_3160_2\_ProjectScratch\</value>
<description>name that has one of the keyword will be kept. Separate the keyword with ","</description>
</property>

<property>
<name>simulator.generatelog.mode</name>
<value>standalone</value>
<description>standalone will run on local machine, "distributed" mode will run on spark. only standalone is supported in this version</description>
</property>

<property>
<name>simulator.log.output.dir</name>
<value>/home/reidchan/Desktop/Simulog_1</value>
<description>the log output directory for this simulation</description>
</property>

<property>
<name>simulator.record.size</name>
<value>50000</value>
<description>the number of record in buffer before flush</description>
</property>

<property>
<name>simulator.modfile.dir</name>
<value>/home/reidchan/Desktop/ModFile</value>
<description>the mod file for simulator</description>
</property>

</configuration>
